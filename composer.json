{
  "name": "enemis/sonata-media-liip-imagine",
  "type": "symfony-bundle",
  "description": "The easiest way to integrate sonata media with liip imagine",
  "keywords": [ "sonata media", "liip imagine", "image", "manipulation", "pictures", "photos", "transformation", "bundle", "symfony" ],
  "homepage": "https://github.com/enemis/sonata-media-liip-imagine",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrey Stadnik",
      "email": "stadnikandreypublic@gmail.com"
    },
    {
      "name": "Sonata Media integration with Liip Imagine"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "symfony/framework-bundle": "~2.3||~3.0",
    "liip/imagine-bundle": "~1.9",
    "sonata-project/media-bundle": "~3.4"
  },
  "require-dev": {
    "symfony/expression-language": "~2.4||~3.0",
    "symfony/phpunit-bridge": "^3.3"
  },
  "autoload": {
    "psr-4": { "Enemis\\SonataMediaLiipImagineBundle\\": "" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}